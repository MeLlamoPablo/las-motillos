{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["./index.js"]
    },
    "check": {
      "dependsOn": ["^check"],
      "inputs": ["**/*.ts"],
      "outputs": ["tsconfig.tsbuildinfo"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    }
  }
}
